<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Main"
            prefHeight="400.0" prefWidth="600.0">

    <Button fx:id="RobotFirst" text="Robot First: NO" onMouseClicked="#RobotFirstButtonClicked" layoutX="100" layoutY="100"></Button>

    <Button fx:id="RobotSecond" text="Robot Second: NO" onMouseClicked="#RobotSecondButtonClicked" layoutX="100" layoutY="150"></Button>

    <Button fx:id="startButton" text="Create Game" onMouseClicked="#startButtonClicked" layoutX="100" layoutY="200"></Button>

</AnchorPane>
